<h2>Restaurants</h2>
<%= link_to "Add a new restaurant", new_restaurant_path%>

<div class ="container">
<% @restaurants.each do |restaurant|%>
<div class="card">
<h3> <%= restaurant.name %></h3>
<p><%= restaurant.category %></p>
<%= link_to "See details", restaurant_path(restaurant)%>
</div>
<% end %>
</div>
